{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "config": {
      "background":"#E8E9F3"
    },
    "title": "title",
    "projection": {"type": "equalEarth"},
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/KaneSec/vega_lite/main/2_symbol_map/js/ne_110m_admin_0_countries.topojson",
          "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
        },
        "mark": {"type": "geoshape", "fill": "#A6A6A8", "stroke": "#A6A6A8"}
      },
      {
        "data": {
          "url":"https://raw.githubusercontent.com/LucaBernardiProjects/FIT3179_A2/master/topojson_plates.topojson",
          "format": {"type": "topojson", "feature":"PB2002_boundaries (1)"}
        },
        "mark": {"type": "geoshape", "fill": "black", "stroke": "#272635", "fillOpacity" : 0}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/LucaBernardiProjects/FIT3179_A2/master/volcano.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "Longitude", "type": "quantitative"},
          "latitude": {"field": "Latitude", "type": "quantitative"},
          "size": {
            "field": "PEI",
            "type": "quantitative",
            "scale": {"domain": [0, 10]}
          },
          "color": {
            "field": "PEI",
            "type": "quantitative",
            "scale": {"scheme": "oranges"}
          }
        }
      }
    ]
  }